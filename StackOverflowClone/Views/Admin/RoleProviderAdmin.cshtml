@model IEnumerable<StackOverflowClone.Models.UserRoles>
@{
    ViewBag.Title = "RoleProviderAdmin";
}

<h2>Admin</h2>
<table class="table table-hover table-bordered">
    <thead>
        <tr>
            <th>Admin Email</th>
            <th>Role</th>
            <th>#</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Username</td>
                <td>@item.Role</td>
                <td>
                    @Html.ActionLink("Edit", "RoleProviderAdminEdit", "Admin", new { id = item.Id }, new { @class = "btn btn-outline-success" })
                </td>
            </tr>

        }
    </tbody>
</table>

