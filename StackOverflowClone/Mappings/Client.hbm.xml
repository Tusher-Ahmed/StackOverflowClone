<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="StackOverflowClone" namespace="StackOverflowClone.Models">
  <class name="Client" table="Client" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="long">
      <generator class="native" />
    </id>

    <property name="Username" />
    <property name="Email" />
    <property name="AboutMe" />
    <property name="Password" />
  </class>
</hibernate-mapping>